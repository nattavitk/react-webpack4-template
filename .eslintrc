{
	"parser": "babel-eslint",
	"extends": ["airbnb", "prettier"],
	"plugins": ["prettier"],
	"env": {
		"browser": true,
		"node": true
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	},
	"rules": {
		"prettier/prettier": ["error"],
		"react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
	}
}
